
        const teks = 'CITA CITA ITU YANG TINGGI';
        const hurufKecil = teks.toLowerCase(); // Mengubah menjadi huruf kecil
        const hurufBesar = teks.toUpperCase(); // Mengubah menjadi huruf besar
        console.log(hurufKecil); // Output: 'cita cita itu yang tinggi'
        console.log(hurufBesar); // Output: 'CiTa CiTa ITU YANG TINGGI'